<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="DedAPI.Views.AllinfoPage">
    <StackLayout>
        <Label Text="{Binding API}"/>
        <Label Text="{Binding Description}"/>
        <Label Text="{Binding Auth}"/>
        <Label x:Name="lbHttps"/>
        <Label Text="{Binding Cors}"/>
        <Label x:Name="lbUrl" Text="{Binding Link}" TextColor="Blue" TextDecorations="Underline" FontAttributes="Bold"/>
        <Label Text="{Binding Category}"/>
    </StackLayout>
</ContentPage>